{
    "name": "arillo/silverstripe-multiselectfield",
    "type": "silverstripe-vendormodule",
    "description": "A sortable multiple select field for managing many-to-many relations",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "multiselect",
        "field",
        "manymany",
        "manytomany",
        "multiselectfield"
    ],
    "homepage": "https://github.com/arillo-ch/silverstripe-multiselectfield",
    "authors": [
        {
            "name": "Arillo Team",
            "email": "hello@arillo.ch"
        }
    ],
    "support": {
        "issues": "https://github.com/arillo-ch/silverstripe-multiselectfield/issues"
    },
    "require": {
        "silverstripe/vendor-plugin": "^2.0 || ^3.0",
        "silverstripe/framework": "^5.0 || ^6.0",
        "arillo/silverstripe-cms-alpine": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Arillo\\MultiSelectField\\": "src/Forms/"
        }
    },
    "extra": {
        "installer-name": "silverstripe-multiselectfield",
        "expose": [
            "client"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
